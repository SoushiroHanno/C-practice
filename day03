#include<stdio.h>
#include<stdlib.h>
int main (void)
{
  int i,n;

  srand(555);
  
  printf("0~2147483647:\n");
  for(i=1;i<=10;i++)
    {
      printf("%d \n", rand());
    }
  
  printf("\n0.0~1.0未満： \n");
  for(i=1; i<=10; i++)
    {
      printf("%5.3f \n", rand() / (RAND_MAX+1.0));
    }
 
  
  printf("\n1~6： \n");
  for(i=1; i<=10; i++)
    {
      n = (rand() % 6);
      printf("%d\n", n);
    }
  printf("\n");
  return 0;
}
